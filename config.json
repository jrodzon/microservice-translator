{
  "base_url": "http://localhost:8000",
  "timeout": 600,
  "startup_timeout": 600,
  "shutdown_timeout": 30,
  "check_interval": 2,
  "output_file": "test_results.json",
  "logging": {
    "level": "DEBUG",
    "file": "logs/project_translator.log",
    "max_file_size": 10485760,
    "backup_count": 5
  },
  "llm_provider": {
    "provider": "openai",
    "model": "gpt-5",
    "api_key": "",
    "base_url": null
  },
  "translation": {
    "method": "batch",
    "max_iterations": 30,
    "save_conversation": false,
    "conversation_file": "translation_conversation.json",
    "conversation_dir": "conversations",
    "auto_save_interval": 3,
    "retry_on_error": true,
    "max_retries": 3,
    "retry_delay": 1.0,
    "enable_auto_testing": true,
    "test_cases_path": "input/test_cases.json"
  }
}